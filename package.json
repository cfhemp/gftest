{
    "name": "gatsby-plugin-gravityforms-component",
    "version": "4.0.0",
    "description": "A component to take GraphQl Gravity Forms query data and return a usable form.",
    "main": "index.js",
    "scripts": {
        "build": "npm-run-all -s clean -p build:*",
        "build:form": "babel src --out-dir ./dist --ignore **/__tests__",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "run-p watch:*",
        "watch:form": "babel src -w --out-dir ./dist --ignore **/__tests__",
        "pretest": "eslint --ignore-path .gitignore . --fix",
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean": "del-cli dist/*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robmarshall/gatsby-gravityforms-component.git"
    },
    "keywords": [
        "gravity",
        "forms",
        "gravityforms",
        "WordPress",
        "form",
        "Gatsby"
    ],
    "author": "Robert Marshall (https://robertmarshall.dev)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/robmarshall/gatsby-plugin-gravityforms-component/issues"
    },
    "homepage": "https://github.com/robmarshall/gatsby-plugin-gravityforms-component#readme",
    "dependencies": {
        "classnames": "^2.3.1",
        "dotenv": "^10.0.0",
        "nanoid": "^3.1.30",
        "oauth-signature": "^1.5.0",
        "prop-types": "^15.7.2",
        "react-google-recaptcha": "^2.1.0",
        "react-hook-form": "^7.17.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.8",
        "babel-eslint": "^10.1.0",
        "babel-preset-gatsby-package": "^2.0.0",
        "cross-env": "^7.0.3",
        "del-cli": "^4.0.1",
        "eslint": "^8.1.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-import-order-alphabetical": "^1.0.1",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-sort-destructure-keys": "^1.3.5",
        "gatsby-plugin-eslint": "^4.0.0",
        "install-peers": "^1.0.3",
        "npm-run-all": "^4.1.5"
    },
    "peerDependencies": {
        "gatsby": "^4.0.0"
    }
}
